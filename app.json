{
  "expo": {
    "name": "MovieKnightApp",
    "slug": "MovieKnightApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon.png",
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.movieknightapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/MK_Icon.png",
        "backgroundColor": "#e63c4b"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
